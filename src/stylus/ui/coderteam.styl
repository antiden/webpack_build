.coderteam
  width 50px
  height 50px
  display flex
  align-items center
  justify-content center
  border-radius 50%
  background @css {rgb(var(--main_color))}
  padding 13px

  svg
    width 100%
    height auto
    fill @css {rgb(var(--main_white))}

  &:hover
    transform scale(1.05)